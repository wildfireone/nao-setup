            angleLists = [x * motion.TO_RAD for x in angleListsDegrees]
            ALMotion.angleInterpolation(joints, angleLists, time, True) 

            
            joints = ['RShoulderRoll', 'RShoulderPitch', 'RElbowRoll', 'RElbowYaw', 'RWristYaw', 'RHand']
